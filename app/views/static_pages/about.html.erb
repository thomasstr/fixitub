<ul class="media-list">
	<li class="media">
		<%= image_tag "aboutus.png", :class => "pull-left" %>
		<div class="media-body">
			<h2 class="media-heading">
				Om oss
			</h2>
			<p></p>
		</div>
	</li>
	<hr>
</ul>
<ul class="thumbnails">
	<% @employees.each do |e| %>
	<li class="span3">
		<div class="thumbnail">
        	<%= image_tag "missing.png" %>
			<div class="caption">
            	<h3><%= e.name %></h3>
                <p><%= e.email %></p>
			</div>
		</div>
	</li>
	<% end %>
</ul>