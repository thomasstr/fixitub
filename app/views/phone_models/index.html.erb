<h2>Telefon-modeller</h2>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @phone_models.each do |phone_model| %>
  <tr>
    <td><%= phone_model.name %></td>
    <td><%= link_to 'Show', phone_model %></td>
    <td><%= link_to 'Edit', edit_phone_model_path(phone_model) %></td>
    <td><%= link_to 'Destroy', phone_model, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

